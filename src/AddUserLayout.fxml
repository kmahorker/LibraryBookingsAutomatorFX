<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="300.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controllers.AddUsersController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label minHeight="-Infinity" minWidth="-Infinity" text="Password" GridPane.rowIndex="2">
         <font>
            <Font size="25.0" />
         </font>
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin>
      </Label>
      <Label alignment="BOTTOM_RIGHT" minHeight="-Infinity" minWidth="-Infinity" text="Add New User" textAlignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
      <Label minHeight="-Infinity" minWidth="-Infinity" text="Username" GridPane.rowIndex="1">
         <font>
            <Font size="25.0" />
         </font>
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
      <TextField fx:id="usernameTextField" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <font>
            <Font size="18.0" />
         </font>
         <GridPane.margin>
            <Insets right="10.0" />
         </GridPane.margin>
      </TextField>
      <PasswordField fx:id="passwordTextField" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <font>
            <Font size="18.0" />
         </font>
         <GridPane.margin>
            <Insets right="10.0" />
         </GridPane.margin>
      </PasswordField>
      <Button fx:id="addButton" alignment="CENTER" mnemonicParsing="false" onAction="#addButtonClicked" text="Add User To Database" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
   </children>
</GridPane>
